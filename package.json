{
  "name": "tiksgiving",
  "description": "Enjoy tiki-themed cocktails for Thanksgiving!",
  "version": "0.0.0",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tiksgiving/tiksgiving.com.git"
  },
  "dependencies": {
    "cross-env": "^5.0.5",
    "electricity": "github:cvan/electricity#a10db6a",
    "express": "^4.16.1",
    "internal-ip": "^3.0.0",
    "shx": "^0.2.2",
    "yonder": "^0.2.0"
  },
  "devDependencies": {
    "live-server": "^1.2.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV='development' live-server public_html --no-browser",
    "prod": "cross-env NODE_ENV='production' node ./server.js",
    "build": "shx rm -rf _build && shx mkdir -p _build && shx cp -R public_html/* _build",
    "preprod": "npm run build"
  }
}
